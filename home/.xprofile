export QT_QPA_PLATFORMTHEME=qt5ct
export PULSE_LOG_FILE=/tmp/pulse.log
export PULSE_LOG=4

# nm-applet to appear as a tray icon
nm-applet &

# Blueman
blueman-applet &

# Automount volumes
udiskie &

# Restore wallpaper
nitrogen --restore &

# Starting a picom - compositor for Xorg
picom -b &

# Screen locker
xss-lock -- bslock &
#light-locker &

# PulseAudio System Tray
pasystray --notify=none &

# EWW
eww daemon || ~/.local/bin/eww daemon &

# Battery tray icon
cbatticon &

# Notifications service
dunst &
